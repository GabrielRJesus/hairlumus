<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MenuPrincipalController">
   <children>
      <MenuBar fx:id="menuBar" layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Gerenciar">
            <items>
              <MenuItem fx:id="menuCliente" mnemonicParsing="false" onAction="#clkCliente" text="Cliente" />
                  <MenuItem fx:id="menuFornecedor" mnemonicParsing="false" onAction="#clkFornecedor" text="Fornecedor" />
                  <MenuItem fx:id="menuFuncionario" mnemonicParsing="false" onAction="#clkFuncionario" text="Funcionario" />
                  <MenuItem fx:id="menuProduto" mnemonicParsing="false" onAction="#clkProduto" text="Produto" />
                  <MenuItem fx:id="menuMarca" mnemonicParsing="false" onAction="#clkMarca" text="Marca" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Fundamentais">
            <items>
                  <MenuItem fx:id="menuVenda" mnemonicParsing="false" onAction="#clkVenda" text="Venda" />
              <MenuItem fx:id="menuCompra" mnemonicParsing="false" onAction="#clkCompra" text="Compra" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Opções">
               <items>
                  <MenuItem fx:id="menuSair" mnemonicParsing="false" onAction="#clkSair" text="Sair" />
               </items></Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
